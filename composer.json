{
    "name": "cedricziel/canva-bundle",
    "description": "A Symfony bundle to integrate canva.com functionality",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Ziel",
            "email": "mail@cedric-ziel.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "cedricziel/canva-extension-helper": "^0.0.9",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/event-dispatcher": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "CedricZiel\\CanvaBundle\\": ""
        }
    },
    "scripts": {
        "test": "@php vendor/bin/simple-phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
